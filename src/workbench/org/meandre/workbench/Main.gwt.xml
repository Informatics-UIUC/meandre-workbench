<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.http.HTTP'/>
	<inherits name='com.google.gwt.user.User'/>
        <inherits name='org.gwtwidgets.WidgetLibrary'/>
        <!-- Inherit gwt-dnd support                            -->
        <inherits name='com.allen_sauer.gwt.dragdrop.DragAndDrop'/>
	<!-- Specify the app entry point class.                   -->
        <!-- Gwm dependency -->
        <inherits name='org.gwm.GwtWindowManager'/>
	<entry-point class='org.meandre.workbench.client.Main'/>
        <servlet path='/WBRepositoryQuery' class='org.meandre.workbench.server.WBRepositoryQueryImpl'/>
</module>
