<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
    <settings defaultResolver="seasrchain"/>
    
    <caches artifactPattern="[artifact]-[revision].[ext]"/>

    <resolvers>
        <chain name="seasrchain">
            <url name="seasr">
                <ivy pattern="http://norma.ncsa.uiuc.edu/public-dav/capitanu/repository/[organisation]/[module]/ivys/ivy-[revision].xml"/>
                <artifact pattern="http://norma.ncsa.uiuc.edu/public-dav/capitanu/repository/[organisation]/[module]/[artifact]-[revision].[ext]"/>
            </url>

            <url name="internal">
                <ivy pattern="http://norma.ncsa.uiuc.edu/public-dav/capitanu/repository/internal/[organisation]/ivys/ivy-[module]-[revision].xml"/>
                <artifact pattern="http://norma.ncsa.uiuc.edu/public-dav/capitanu/repository/internal/[organisation]/[artifact]-[revision].[ext]"/>
            </url>
            
            <ibiblio name="ibiblio" m2compatible="true"/>
        </chain>
    </resolvers>
</ivysettings>
